; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={
}

[application]

config/name="Alchemy"
run/main_scene="res://tscn/main.tscn"

[autoload]

Global="*res://gd/global.gd"
Drag="*res://gd/drag.gd"

[display]

window/size/height=1024
window/size/resizable=false
window/size/fullscreen=true
window/vsync/use_vsync=false
window/per_pixel_transparency/allowed=true
window/per_pixel_transparency/enabled=true
window/stretch/mode="2d"
window/stretch/aspect="keep_width"

[editor]

script_templates_search_path="res://gd/templates"

[input_devices]

pointing/emulate_touch_from_mouse=true
pointing/emulate_mouse_from_touch=false

[node]

name_num_separator=2
name_casing=2

[rendering]

environment/default_clear_color=Color( 0.0313726, 0.0313726, 0.0313726, 1 )
quality/filters/msaa=2
quality/depth/hdr=false
environment/default_environment="res://res/default_env.tres"
